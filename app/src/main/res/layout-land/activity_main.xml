<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">
    <ImageView
            android:layout_width="0dp"
            android:layout_height="120dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView21"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" android:layout_marginTop="108dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/imageView21"
            android:layout_marginStart="32dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView27"
            app:layout_constraintStart_toEndOf="@+id/logo" app:layout_constraintHorizontal_bias="0.95"
            app:layout_constraintEnd_toStartOf="@+id/imageView28" app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView28"
            app:layout_constraintStart_toEndOf="@+id/imageView27" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView29" app:layout_constraintHorizontal_chainStyle="packed"
            android:layout_marginEnd="32dp" android:layout_marginStart="32dp"
            app:layout_constraintTop_toTopOf="@+id/imageView29"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView29"
            app:layout_constraintStart_toEndOf="@+id/imageView28" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView21"/>
    <TextView
            android:text="@string/devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="18sp"
            android:textColor="#0E0D0D" android:textStyle="bold" android:typeface="monospace"
            app:layout_constraintStart_toEndOf="@+id/logo" android:layout_marginStart="24dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/imageView21"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView30"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView31" app:layout_constraintHorizontal_chainStyle="packed"
            android:layout_marginEnd="16dp"
            app:layout_constraintTop_toTopOf="@+id/imageView31"
            app:layout_constraintBottom_toBottomOf="@+id/imageView31" app:layout_constraintDimensionRatio="1:1"
            android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView31"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toEndOf="@+id/imageView30" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView32" app:layout_constraintHorizontal_chainStyle="packed"
            android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
            app:layout_constraintTop_toTopOf="@+id/imageView32"
            app:layout_constraintBottom_toBottomOf="@+id/imageView32" app:layout_constraintDimensionRatio="1:1"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView32"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toEndOf="@+id/imageView31" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView33" app:layout_constraintHorizontal_chainStyle="packed"
            android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
            app:layout_constraintTop_toTopOf="@+id/imageView33"
            app:layout_constraintBottom_toBottomOf="@+id/imageView33" app:layout_constraintDimensionRatio="1:1"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView33"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toEndOf="@+id/imageView32"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed" android:layout_marginStart="16dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/logo"
            app:layout_constraintDimensionRatio="1:1" android:layout_marginEnd="16dp"/>
</android.support.constraint.ConstraintLayout>